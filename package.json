{
  "name": "@srsholmes/gentests",
  "version": "0.0.1",
  "description": "Automatically generate unit test templates",
  "main": "index.js",
  "scripts": {
    "test": "jest ./src --watch",
    "build": "tsc",
    "build:watch": "tsc -w",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\""
  },
  "prettier": {
    "singleQuote": true
  },
  "keywords": [
    "ava",
    "enzyme",
    "jest",
    "react",
    "react-testing-library",
    "tape",
    "test",
    "testing",
    "unit"
  ],
  "author": "Simon Holmes",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/parser": "^7.7.3",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.7.2",
    "@babel/traverse": "^7.7.2",
    "chalk": "^3.0.0",
    "cli-highlight": "^2.1.4",
    "fast-glob": "^3.1.0",
    "typescript": "^3.7.2"
  },
  "devDependencies": {
    "@babel/types": "^7.7.2",
    "@types/jest": "^24.0.12",
    "@types/node": "^12.12.14",
    "@types/prettier": "^1.18.3",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "eslint": "^6.7.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.3.0",
    "nodemon": "^2.0.1",
    "prettier": "^1.16.4"
  }
}
