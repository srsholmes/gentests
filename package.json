    "build:watch": "tsc -w",
    "lint:list": "eslint */**/*.js --fix",
    "lint": "eslint */**/*.js --fix",
    "start:prod": "nodemon src/index.ts",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\""
  },
  "prettier": {
    "singleQuote": true
  },
  "keywords": [],
